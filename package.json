{
  "name": "vue-bind-once",
  "version": "0.1.1",
  "description": "A tiny, SSR-safe directive for binding random data to an element.",
  "keywords": [
    "vue",
    "directive",
    "ssr"
  ],
  "repository": "danielroe/vue-bind-once",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "pnpm lint:all:eslint && pnpm lint:all:prettier",
    "lint:all:eslint": "pnpm lint:eslint --ext .js,.ts .",
    "lint:all:prettier": "pnpm lint:prettier \"{src,test}/**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "husky install && pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test && pinst --disable",
    "release": "release-it",
    "test": "vitest run",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "scule": "^0.2.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "c8": "^7.11.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "esbuild": "0.14.29",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.13.0",
    "happy-dom": "^2.53.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.1",
    "release-it": "^14.13.1",
    "semver": "^7.3.5",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.8.1",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "build": {
    "externals": [
      "vue"
    ]
  },
  "packageManager": "pnpm@6.32.2"
}
