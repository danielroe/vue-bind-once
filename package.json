{
  "name": "vue-bind-once",
  "version": "0.1.1",
  "description": "A tiny, SSR-safe directive for binding random data to an element.",
  "keywords": [
    "vue",
    "directive",
    "ssr"
  ],
  "repository": "danielroe/vue-bind-once",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "npm run lint:all:eslint && npm run lint:all:prettier",
    "lint:all:eslint": "npm run lint:eslint -- --ext .js,.ts .",
    "lint:all:prettier": "npm run lint:prettier -- \"{src,test}/**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run lint && npm run test && pinst --disable",
    "release": "release-it",
    "test": "vitest run",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "scule": "^0.2.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "esbuild": "0.14.45",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.13.0",
    "happy-dom": "^5.3.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "release-it": "^15.0.0",
    "semver": "^7.3.7",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "build": {
    "externals": [
      "vue"
    ]
  },
  "packageManager": "pnpm@7.2.1"
}
