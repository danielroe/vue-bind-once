{
  "name": "vue-bind-once",
  "version": "0.2.1",
  "description": "A tiny, SSR-safe directive for binding random data to an element.",
  "keywords": [
    "vue",
    "directive",
    "ssr"
  ],
  "repository": "danielroe/vue-bind-once",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "npm run lint:all:eslint && npm run lint:all:prettier",
    "lint:all:eslint": "npm run lint:eslint -- --ext .js,.ts .",
    "lint:all:prettier": "npm run lint:prettier -- \"{src,test}/**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "release": "bumpp && npm publish",
    "test": "vitest run"
  },
  "dependencies": {
    "scule": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vitest/coverage-v8": "0.32.0",
    "@vue/test-utils": "2.3.2",
    "bumpp": "9.1.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "esbuild": "0.18.4",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect-type": "0.16.0",
    "happy-dom": "9.20.3",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "5.1.3",
    "unbuild": "1.2.1",
    "vite": "4.3.9",
    "vitest": "0.32.0",
    "vue": "3.3.4"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "build": {
    "externals": [
      "vue"
    ]
  },
  "packageManager": "pnpm@8.6.2"
}
